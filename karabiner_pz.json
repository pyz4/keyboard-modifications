{
    "title": "PZ keyboard modifications",
    "rules": [
        {
            "description": "rshift+spacebar to underscore",
            "manipulators": [{
                "from": {
                    "key_code": "spacebar",
                    "modifiers": {
                        "mandatory": [
                            "right_shift"
                        ],
                        "optional": [
                            "any"
                        ]
                    }
                },
                "to": [{
                    "key_code": "hyphen",
                    "modifiers": [
                        "left_shift"
                    ]
                }],
                "type": "basic"
            }]
        },
        {
            "description": "ESDF Navigation",
            "manipulators": [{
                    "from": {
                        "key_code": "e",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "up_arrow"
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "d",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "down_arrow"
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "s",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "left_arrow"
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "f",
                        "modifiers": {
                            "mandatory": [
                                "right_control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "right_arrow"
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "s",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "left_arrow",
                        "modifiers": [
                            "right_option"
                        ]
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "f",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "right_arrow",
                        "modifiers": [
                            "right_option"
                        ]
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "d",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "down_arrow",
                        "modifiers": [
                            "right_option"
                        ]
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "e",
                        "modifiers": {
                            "mandatory": [
                                "right_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "up_arrow",
                        "modifiers": [
                            "right_option"
                        ]
                    }],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Chain Modifiers",
            "manipulators": [{
                    "from": {
                        "simultaneous": [{
                                "key_code": "f"
                            },
                            {
                                "key_code": "w"
                            }
                        ]
                    },
                    "to": [{
                        "key_code": "left_option"
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "simultaneous": [{
                                "key_code": "f"
                            },
                            {
                                "key_code": "e"
                            }
                        ]
                    },
                    "to": [{
                        "key_code": "left_control"
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "simultaneous": [{
                                "key_code": "j"
                            },
                            {
                                "key_code": "o"
                            }
                        ]
                    },
                    "to": [{
                        "key_code": "right_option"
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "simultaneous": [{
                                "key_code": "j"
                            },
                            {
                                "key_code": "i"
                            }
                        ]
                    },
                    "to": [{
                        "key_code": "right_control"
                    }],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "IKOL Deletions",
            "manipulators": [{
                    "from": {
                        "key_code": "l",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "delete_or_backspace"
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "o",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "delete_or_backspace",
                        "modifiers": [
                            "left_option"
                        ]
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "k",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "delete_or_backspace",
                        "modifiers": [
                            "fn"
                        ]
                    }],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "i",
                        "modifiers": {
                            "mandatory": [
                                "left_option"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [{
                        "key_code": "delete_or_backspace",
                        "modifiers": [
                            "left_option",
                            "fn"
                        ]
                    }],
                    "type": "basic"
                }
            ]
        }
    ]
}
